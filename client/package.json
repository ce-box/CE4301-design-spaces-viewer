  "description": "Data visualization for GEM5 stats",
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.8",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^6.4.0",
    "electron-is-dev": "^2.0.0",
    "electron-util": "^0.17.2",
    "electronmon": "^2.0.2",
    "frameless-titlebar": "^2.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "^4.5.2",
    "wait-on": "^6.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run react-serve\" \"wait-on http://localhost:3000 && npm run electron-start\"",
    "build": "npm run react-build && npm run electron-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "npm run react-build && npm run electron-release",
    "electron-start": "electronmon .",
    "electron-build": "electron-builder",
    "electron-release": "electron-builder --publish=always",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-serve": "SET BROWSER=none&&npm run react-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "homepage": "./",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron": "^16.0.1",
    "electron-builder": "^22.13.1"
  }
